package Assignment341;

public class Assignment341 {
			public static void main(String args[]){
				details d1=new details("a",123);
				d1.showDetails();
				details d2=new details("b",456);
				d2.showDetails();
				details.colonyShifted();
				details d3=new details("c",789);
				d3.showDetails();
			} 
		}
		class details{
			String name;
			int houseno;
			static String colony="sattenapalli";
			
			static void colonyShifted(){
				colony="local";
			}
			details(String owner,int drno){
				name=owner;
				houseno=drno;
			}
			void showDetails(){
				System.out.println("Name: "+name+" "+"houseno: "+houseno+" "+"colony:"+colony);
			}
		}
		
