package Assignment342;

public class Assignment342{
	Assignment342()
		    {
		        this(15);
		        System.out.println("The Default constructor");
		    }
	Assignment342(int x)
		    {
		        this(15, 5);
		        System.out.println(x);
		    }
	Assignment342(int x, int y)
		    {
		        System.out.println(x * y);
		    }
		 
		    public static void main(String args[])
		    {
		        new Assignment342();
	

	}

}
